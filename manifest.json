{
  "manifest_version": 2,

  "name": "DontKnowVideo",
  "description": "This extension shows a video shortcut when you search video in serach engine",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Dont Know Video?"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}